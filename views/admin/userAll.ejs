 <div class="wrapper userProfile">
    <%- include('../error/flashMessages') %>
    <table class="table table-dark table-striped">
        <tr>
           <th>#</th><th>Name</th><th>UserName</th><th>Email</th><th>Phone</th><th>Work</th><th>city</th><th></th>
        </tr>
     
        <% for (var i = 0; i < users.length; i++) { %>
         <tr>
            <td><%=i+1%></td>
           <td><%= users[i].name %></td>
           <td><%= users[i].username %></td>
           <td><%= users[i].email%></td>
           <td><%= users[i].userInfo.mobile %></td>
           <td><%= users[i].userInfo.work %></td>
           <td><%= users[i].userInfo.city%></td>
           <% if(users[i].role == 0) {%>
            <td><a class="btn btn-danger" href=<%='/lock/'+users[i].username %> >Lock</a></td>
            <%} else if (users[i].role == 2){%>
                <td><a class="btn btn-warning" href=<%='/unlock/'+users[i].username %> >UnLock</a></td>
            <%} else {%>
                <td>Admin</td>
            <%}%>
         </